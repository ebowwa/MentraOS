			pdm0: dmic_dev: pdm@26000 {
				compatible = "nordic,nrf-pdm";
				reg = < 0x26000 0x1000 >;
				interrupts = < 0x26 0x1 >;
				status = "okay";
				pinctrl-0 = < &pdm0_default_alt >;
